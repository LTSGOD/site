{
    "name": "jiny/site",
    "description": "jinySite for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "jiny/laravel": "^0.7",

        "jiny/icons": "^0.3.1",
        "jiny/theme": "^0.7",

        "jiny/menu": "^0.5",
        "jiny/posts": "^0.0.3",
        "jiny/markdown": "^0.2",
        "jiny/frontmatter": "^0.1.3",
        "webuni/front-matter": "^2.0",

        "jiny/site-board": "^0.4",
        "jiny/site-cms": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Site\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Site\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Site\\JinySiteServiceProvider"
            ]
        }
    }

}
